model YearInReview {
  id           String    @id @default(cuid())
  year         Int
  totalLinks   Int
  totalClicks  Int
  topLinks     Json
  topCountries Json
  workspaceId  String
  createdAt    DateTime  @default(now())
  sentAt       DateTime?
  workspace    Project   @relation(fields: [workspaceId], references: [id])

  @@index([workspaceId])
}
